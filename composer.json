{
    "name": "dpetelin/doctrine-enum-bundle",
    "description": "Provides support of ENUM type for Doctrine2 in Symfony2 applications.",
    "keywords": ["bundle", "symfony", "doctrine", "entity", "enum", "type", "DBAL", "MySQL", "SQLite", "PostgreSQL", "MSSQL"],
    "homepage": "https://github.com/dpetelin/DoctrineEnumBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Genvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Dmitry Petelin",
            "email": "dpetelin@issart.com",
            "role": "Editor"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/symfony": "~2.6|~3.0",
        "doctrine/orm": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "~2.5",
        "escapestudios/symfony2-coding-standard": "~2.8"
